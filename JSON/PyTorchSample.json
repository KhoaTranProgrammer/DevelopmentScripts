{
    "Introduction":
    {
        "Description":
        {
            "list":[
                "This is example of testing",
                "python ControlProcess.py -ji JSON/PyTorchSample.json"
            ]
        }
    },
    "PyTorch":
    {
        "Transforms":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "train_transform",
                        "Rotation": "30",
                        "ResizedCrop": "32",
                        "HorizontalFlip": true
                    }
                },
                {
                    "Input":
                    {
                        "ID": "test_transform"
                    }
                }
            ]
        },
        "DataLoader":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "train_loader",
                        "DataType": "CIFAR10",
                        "Location": "{BUILD}/CIFAR10/",
                        "Transforms": "train_transform",
                        "Download": false,
                        "Train": true,
                        "BatchSize": "32"
                    }
                },
                {
                    "Input":
                    {
                        "ID": "test_loader",
                        "DataType": "CIFAR10",
                        "Location": "{BUILD}/CIFAR10/",
                        "Transforms": "test_transform",
                        "Download": true,
                        "Train": false,
                        "BatchSize": "32"
                    }
                }
            ]
        },
        "ShowImages":
        {
            "list":[
                {
                    "Input":
                    {
                        "DataLoader": "train_loader",
                        "Show": false,
                        "Count": "1",
                        "Classes": ["plane", "car", "bird", "cat", "deer", "dog", "frog", "horse", "ship", "truck"]
                    }
                }
            ]
        },
        "NeuralNetwork":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "neural_network",
                        "DefinedNet": "{SCRIPTS}/JSON/PyTorchSampleNeuralNetwork.py"
                    }
                }
            ]
        },
        "Criterion":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "criterion",
                        "Loss": "NLLLoss"
                    }
                }
            ]
        },
        "Optimizer":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "optimizer",
                        "NeuralNetwork": "neural_network",
                        "Optim": "SGD",
                        "LR": "0.003",
                        "Momentum": "0.9"
                    }
                }
            ]
        },
        "Training":
        {
            "list":[
                {
                    "Input":
                    {
                        "NeuralNetwork": "neural_network",
                        "Criterion": "criterion",
                        "Optimizer": "optimizer",
                        "TrainLoader": "train_loader",
                        "ValidationLoader": "test_loader",
                        "Epochs": "1"
                    }
                }
            ]
        },
        "Testing":
        {
            "list":[
                {
                    "Input":
                    {
                        "NeuralNetwork": "neural_network"
                    }
                }
            ]
        }
    }
}
