{
    "Introduction":
    {
        "Description":
        {
            "list":[
                "This is example of testing",
                "python ControlProcess.py -ji JSON/PyTorchSample.json"
            ]
        }
    },
    "PyTorch":
    {
        "Transforms":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "train_transform",
                        "Rotation": "30",
                        "ResizedCrop": "32",
                        "HorizontalFlip": true
                    }
                }
            ]
        },
        "DataLoader":
        {
            "list":[
                {
                    "Input":
                    {
                        "ID": "train_loader",
                        "DataType": "CIFAR10",
                        "Location": "{BUILD}/CIFAR10/",
                        "Transforms": "train_transform",
                        "Download": false,
                        "Train": true,
                        "BatchSize": "32"
                    }
                }
            ]
        },
        "ShowImages":
        {
            "list":[
                {
                    "Input":
                    {
                        "DataLoader": "train_loader",
                        "Show": false,
                        "Count": "1",
                        "Classes": ["plane", "car", "bird", "cat", "deer", "dog", "frog", "horse", "ship", "truck"]
                    }
                }
            ]
        }
    }
}
